@inherits Umbraco.Web.Mvc.UmbracoViewPage
@using ClientDependency.Core.Mvc

@{
	Layout = null;

	Html.RequiresCss("/vendor/bootstrap/css/bootstrap.min.css");
	Html.RequiresCss("https://fonts.googleapis.com/css?family=Raleway:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i");
	Html.RequiresCss("https://fonts.googleapis.com/css?family=Lora:400,400i,700,700i");
	Html.RequiresCss("/css/business-casual.min");

	Html.RequiresJs("/vendor/jquery/jquery.min.js");
	Html.RequiresJs("/vendor/bootstrap/js/bootstrap.bundle.min.js");
}

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="description" content="">
	<meta name="author" content="">
	<title>Coffee Shop</title>
	@Html.RenderCssHere()
</head>

<body>
	@Html.Partial("~/Views/Partials/_Header.cshtml")
	@Html.Partial("~/Views/Partials/_Navbar.cshtml")
	@RenderBody()
	<hr>
	@Html.Partial("~/Views/Partials/_Footer.cshtml")
	@Html.RenderJsHere()
	@RenderSection("ScriptsBottom", false)
</body>

</html>
